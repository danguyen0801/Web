                      <div class="input-field">
                        <div class="input-select">
                          <select data-trigger="" name="choices-single-defaul">
                            <option placeholder="" value="">Màu sắc</option>
                            <option>Đỏ</option>
                            <option>Xanh </option>
                          </select>
                        </div>
                      </div>
                      <div class="input-field">
                        <div class="input-select">
                          <select data-trigger="" name="choices-single-defaul">
                            <option placeholder="" value="">Kích cỡ </option>
                            <option> Lớn </option>
                            <option> Nhỏ </option>
                          </select>
                        </div>
                      </div>
                    </div>
                    <div class="row second">
                      <div class="input-field">
                        <div class="input-select">
                          <select data-trigger="" name="choices-single-defaul">
                            <option placeholder="" value="">Khuyến mãi </option>
